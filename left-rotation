#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,d,i,j,temp;
    scanf("%d %d",&n,&d);
    int arr[n];
    for(i=0;i<n;i++)
        scanf("%d ",&arr[i]);
    if(d<n){
    for(i=d;i<n;i++)
        printf("%d ",arr[i]);
    for(i=0;i<d;i++)
        printf("%d ",arr[i]);
    }
    else
    for(int i=n-1;i>=0;i--){
    printf("%d ",arr[i]);
    }
    return 0;
}
